{
	"$schema": "https://json.schemastore.org/web-manifest-combined.json",
	"id": "com.example.xstackdemo",
	"short_name": "X Stack",
	"name": "X Stack Demo",
	"start_url": ".",
	"display": "standalone",
	"theme_color": "#000",
	"background_color": "#fff",
	"description": "A demo of the X Stack",
	"icons": [
	  {
		"src": "/icons/X-light/android-chrome-192x192.png",
		"sizes": "192x192",
		"type": "image/png"
	  },
	  {
		"src": "/icons/X-light/android-chrome-512x512.png",
		"sizes": "512x512",
		"type": "image/png",
		"purpose": "any"
	  },
	  {
		"src": "/icons/X-light/apple-touch-icon.png",
		"sizes": "180x180",
		"type": "image/png"
	  },
	  {
		"src": "/icons/X-light/favicon-16x16.png",
		"sizes": "16x16",
		"type": "image/png"
	  },
	  {
		"src": "/icons/X-light/favicon-32x32.png",
		"sizes": "32x32",
		"type": "image/png"
	  },
	  {
		"src": "/icons/X-light/mstile-150x150.png",
		"sizes": "150x150",
		"type": "image/png"
	  }
	],
	"display_override": [
	  "fullscreen",
	  "standalone",
	  "window-controls-overlay"
	],
	"orientation": "portrait"
}
